.noorderview {
  display: flex;
  flex-direction: column;
}

.backimage {
  margin: 30% 35% 0 35%;
  width: 30%;
  height: 300rpx;
}

.title1 {
  margin: 5% 30% 0 30%;
  width: 40%;
  height: 40rpx;
  text-align: center;
  font-size: 34rpx;
  color: #666;
}

.title2 {
  margin: 5% 0 0 0;
  width: 100%;
  height: 40rpx;
  text-align: center;
  color: #666;
  font-size: 30rpx;
}

.gobuy {
  margin: 5% 15% 0 15%;
  border: 1px solid #666;
  border-radius: 10rpx;
  height: 80rpx;
  position: relative;
}

.gobuy-title {
  top: 15rpx;
  left: 0;
  height: 40rpx;
  width: 100%;
  position: absolute;
  font-size: 40rpx;
  text-align: center;
  color: #666;
}