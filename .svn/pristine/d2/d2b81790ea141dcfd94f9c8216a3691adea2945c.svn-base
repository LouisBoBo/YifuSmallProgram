}

.discription {
  top: 0;
  left: 0;
  position: absolute;
  width: 100%;
  height: 350rpx;
  display: flex;
  flex-direction: column;
}

.discription-title {
  text-align: center;
  color: white;
  font-size: 34rpx;
  width: 90%;
  height: 80rpx;
  margin-top: 200rpx;
  margin-left: 5%;
}

.discription-content {
  text-align: center;
  color: white;
  font-size: 30rpx;
  width: 90%;
  height: 80rpx;
  margin-left: 5%;
}

.page-section-spacing {
  margin-right: 10rpx;
}

.scroll-view_H {
  white-space: nowrap;
}

.scroll-view-item {
  height: 330rpx;
}

.scroll-view-item_H {
  margin-left: 10rpx;
  display: inline-block;
  width: 200rpx;
  height: 330rpx;
}

.scroll-view-item_H image {
  width: 200rpx;
  height: 200rpx;
}

.scroll-view-item_H_name {
  width: 200rpx;
  height: 40rpx;
  margin-top: 10rpx;
  vertical-align: middle;
  text-align: center;
  font-size: 28rpx;
  color: #666;
  background: white;
  position: absolute;
}

.scroll-view-item_H_price {
  width: 200rpx;
  height: 40rpx;
  position: absolute;
  margin-left: 0rpx;
  top: 280rpx;
  opacity: 0.6;
  vertical-align: middle;
  text-align: center;
  font-size: 32rpx;
  color: #ff3f8b;
}

.view-xinyijie {
  position: relative;
  width: 630rpx;
  background: rgba(255, 255, 255, 0.0);
  border-radius: 10rpx;
  display: flex;
