
<!--商品热门搜索  -->

<import src="../wxSearch/wxSearch.wxml" />


<!--搜索按钮  -->
<view class="wxSearch-section">
  <view class="wxSearch-pancel">
    <input bindinput="wxSearchInput" bindfocus="wxSerchFocus" value="{{wxSearchData.value}}" bindblur="wxSearchBlur" class="wxSearch-input" placeholder="{{searchPlacehorder}}" placeholder-style='color: #a8a8a8' 
    
    confirm-type="done"
    bindconfirm="wxSearchFn"
    
    />
    <button class="wxSearch-button" bindtap="wxSearchFn" size="mini" plain="true">搜索</button>
  </view>
</view>
 <template is="wxSearch" data="{{wxSearchData}}" /> 