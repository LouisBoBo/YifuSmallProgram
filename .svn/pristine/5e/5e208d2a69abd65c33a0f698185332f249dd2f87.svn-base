.fightView{
  width: 580rpx;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
  padding-top: 60rpx;
}
.fightView-title{
  align-items: center;
  display: flex;
  flex-direction: row;
}
.fightView-title-text{
  margin-left: 10rpx;
  text-align: center;
  font-size: 30rpx;
  color: white;
}
.fightView-title-imagenew{
  background-color: white;
  align-items: center;
  margin-left: 90rpx;
  border-radius: 20rpx;
  width: 40rpx;
  height: 40rpx; 
}
.fightView-title-image{
  background-color: white;
  align-items: center;
  margin-left: 200rpx;
  border-radius: 20rpx;
  width: 40rpx;
  height: 40rpx;
}
.fightView-content{
  margin-top: 20rpx;
  text-align: center;
  font-size: 30rpx;
  color: white;
}
.fightView-content-number{
  font-size: 32rpx;
  color: #ff3f8b;
}
.fightView-share{
  margin: 50rpx 100rpx 0rpx 100rpx;
  height: 150rpx;
  display: flex;
  flex-direction: row;
}
.zw-tabs-item {
  flex-direction: column;
  display: flex;
  flex: 1;
  align-items: center;
  padding: 0 0 0 0;
}

.zw-tabs-item image {
  width: 100rpx;
  height: 100rpx;
  padding-top: 5px;
  padding-bottom: 2px;
  position: relative;
}

.zw-tabs-item text {
  color:white;
  font-size: 10px;
  margin-top: 10rpx;
}
.weixin-share-icon-btn {
  left: 220rpx;
  width: 300rpx;
  height: 140rpx;
  opacity: 0;
  background-color: green;
  position: absolute;
}
.KfightView-shareimage{
  display: flex;
  margin-top: 30rpx;
  margin-left: 5%;
  margin-right: 5%;
  position: relative;
  align-items: center;
  justify-content: center;
}
.fightView-shareimage{
  display: flex;
  margin-top: 30rpx;
  margin-left: 10%;
  margin-right: 10%;
  position: relative;
  align-items: center;
  justify-content: center;
  /* height: 500rpx; */
}
.KfightView-shareimage-image{
  width: 110%;
}
.fightView-shareimage-image{
  width: 100%;
  /* height: 100%; */
}
.fightView-shareimage-text{
  position: absolute;
  margin-top: 170rpx;
  margin-left: -345rpx;
  text-align: center;
  font-size: 36rpx;
  color: white;
  width: 250rpx;
  font-weight: 600;
}

.fightView-shareimage-button{
  position: absolute;
  margin-top: 260rpx;
  margin-left: -345rpx;
  text-align: center;
  font-size: 20rpx;
  color: white;
  width: 250rpx;
  height: 60rpx;
  border-radius: 5rpx;
  background-color: yellow;
}
.fightView-shareimage-buttontitle{
  position: absolute;
  margin-top: 270rpx;
  margin-left: -345rpx;
  text-align: center;
  font-size: 30rpx;
  color: red;
  width: 250rpx;
}


.fightView-shareimage-button1{
  position: absolute;
  margin-top: 180rpx;
  justify-content: center;
  align-content: center;
  width: 120rpx;
  height: 120rpx;
}
.fightView-shareimage-buttontitle1{
  position: absolute;
  margin-top: 360rpx;
  /* margin-left: -345rpx; */
  text-align: center;
  justify-content: center;
  align-content: center;
  width: 120rpx;
  height: 120rpx;
}

.fightView_tixiantime{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-content: center;
  text-align: center;
  width: 100%;
  height: 100px;
  position: absolute;
  margin-top:-50rpx;
  color: rgb(255, 222, 0);
  font-size: 24rpx;
}
.fightView-close{
  margin-top: 40rpx;
  margin-left: 260rpx;
  margin-bottom: 60rpx;
  width: 60rpx;
  height: 60rpx;
  border-radius: 30rpx;
  align-content: center;
}