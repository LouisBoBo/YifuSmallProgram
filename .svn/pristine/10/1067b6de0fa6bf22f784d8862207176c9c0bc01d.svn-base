@import "../../../common/FightShop-template/FightShopShare.wxss";
@import "../../../common/Freelingkefu-template/Freelingkefu.wxss";

.mainView {
  display: flex;
  flex-direction: column;
  overflow: hidden;
}

.mengcheng {
  width: 200%;
  height: 100%;
  margin-left: -50rpx;
  position: absolute;
  background-color: rgba(0, 0, 0, 0.7);
}

.fight_Share {
  display: flex;
  flex-direction: column;
}

.line {
  display: flex;
  flex-direction: row;
  width: 714rpx;
  height: 2rpx;
  background-color: #e3e3e3;
  align-self: center;
  margin-top: -30rpx;
}
.line-xuxian{
  width: 714rpx;
  height: 0.1rpx;
  border-top: 2rpx dashed #e3e3e3;
  align-self: center;
  margin-top: 19rpx;


}

.fight_Share_title {
  margin-top: 58rpx;
  margin-bottom: 49rpx;
  font-size: 37rpx;
  justify-content: center;
  color: black;
  display: flex;
}

.fight_Share_title2 {
  margin-top: 23rpx;
  font-size: 32rpx;
  justify-content: center;
  color: black;
  display: flex;
}

.fight_Share_title3 {
  margin-top: 43rpx;
  font-size: 37rpx;
  justify-content: center;
  color: black;
  display: flex;
  margin-bottom: 39rpx;
}

.fight_Share_title-tips {
  margin-top: 25rpx;
  font-size: 26rpx;
  justify-content: center;
  color: black;
  display: flex;
}

.fight_Share_view {
  margin: 0rpx 0 38rpx 0;
  width: 100%;
  align-items: center;
}

.fight_Share_button {
  width: 67%;
  height: 80rpx;
  background-color: #f45f4b;
  border-radius: 40rpx;
  font-weight: bold;
  color: white;
  display: flex;
  justify-content: center;
  align-items: center;
}

.fight_Share_view {
  display: flex;
  flex-direction: column;
}

.fight_Share_view_title1 {
  margin-top: 10rpx;
  margin-left: 20rpx;
  color: white;
  font-size: 24rpx;
  height: 40rpx;
}

.fight_Share_view_title2 {
  margin-top: 0rpx;
  margin-left: 20rpx;
  margin-bottom: 0rpx;
  color: white;
  font-size: 24rpx;
  display: flex;
  justify-content: center;
  height: 40rpx;
}

.fight_Share_view_swiper {
  margin-top: 10rpx;
  height: 715rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
}

.fight_Share_view_swiper-image {
  height: 100%;
  width: 100%;
}

.fight_Share_view_button {
  margin-top: 20rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
  height: 70rpx;
}

.fight_Share_view_button_left {
  display: flex;
  margin-bottom: 10rpx;
  height: 45rpx;
  color: white;
  border: 1px solid #f0f0f0;
  border-top: 1px solid #f0f0f0;
  font-size: 26rpx;
  justify-content: center;
  align-items: center;
  float: left;
  padding: 0 10rpx;
}

.fight_Share_view_button_right {
  display: flex;
  margin-bottom: 10rpx;
  height: 45rpx;
  color: white;
  border: 1px solid #f0f0f0;
  border-top: 1px solid #f0f0f0;
  font-size: 26rpx;
  justify-content: center;
  align-items: center;
  float: right;
  padding: 0 10rpx;
}

.fight_list {
  width: 95%;
  height: auto;
  display: flex;
  flex-direction: column;
  align-self: center;
  background-color: white;
  border-radius: 5px;
  margin-top: 38rpx;
  margin-bottom: 0rpx;
  position: relative;
}

.list-scoll {
  height: 501px;
  display: flex;
  flex-direction: column;
  margin-bottom: 5rpx;
}

.list-scoll-cover {
  height: 480px;
  width: 100%;
  display: flex;
  flex-direction: column;
  margin-bottom: 5rpx;
  background-color: rgba(0, 0, 0, 0.0);
  position: absolute;
  top: 0;
  left: 0;
  margin-top: 100rpx;
}

.list-item {
  height: 60px;
  display: flex;
  flex-direction: row;
  align-items: center;
  background-color: white;
  border-top: 0.5px solid #c5c5c5;
  padding-left: 20rpx;
  padding-right: 20rpx;
}

.list-item image {
  width: 42px;
  height: 42px;
  border-radius: 50%;
  align-self: center;
}

.user-name {
  font-size: 12px;
  color: #7d7d7d;
  margin-left: 20rpx;
  margin-right: 20rpx;
}

.user-content {
  font-size: 12px;
  color: #7d7d7d;
  margin-right: 10rpx;
  width: 300rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.user-money {
  font-size: 14px;
  color: #ff3f8b;
  position: absolute;
  right: 20rpx;
}

.drawer_screen_withdraw3 {
  display: flex;
  width: 100%;
  height: 100%;
  justify-content: center;
  align-items: center;
  position: fixed;
  z-index: 999;
  top: 0;
  background-color: rgba(0, 0, 0, 0.93);
}

.titleview {
  margin-bottom: 17rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.titleview-image {
  margin-top: 7rpx;
  height: 22rpx;
  width: 26rpx;
}

.titleview-title {
  display: flex;
  justify-content: center;
  font-size: 26rpx;
  margin-left: 15rpx;
}

.gitbaseview-view {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  align-content: center;
  width: 510rpx;
  height: 905rpx;
  border: 1px solid rgb(236, 145, 191);
  border-radius: 45rpx;
  position: relative;
  background: #eee;
}

.gitbaseview-view-loadimg {
  height: 80rpx;
  width: 80rpx;
  display: flex;
  justify-content: center;
  align-content: center;
}

.gitbaseview-image {
  width: 510rpx;
  height: 905rpx;
  border: 1px solid rgb(236, 145, 191);
  border-radius: 45rpx;
  position: absolute;
  justify-content: center;
  align-content: center;
  z-index: 99;
  background: white;
}

.progress-long {
  margin-top: 23rpx;
  width: 449rpx;
  align-self: center;
  border-radius: 15px;
  border: 1px solid transparent;
  overflow: hidden;
}

.video {
  width: 100%;
  height: 100%;
  margin: 0;
  border-radius: 40rpx;
}

.bottom-center {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.bottom-center-out-left {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  width: 100%;
}

.share-cover {
  position: relative;
  height: 92rpx;
  width: 501rpx;
  border-radius: 10rpx;
  background-color: #49c50d;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.weixin-share-icon-btn {
  left: 0;
  width: 501rpx;
  height: 92rpx;
  opacity: 0;
  background: red;
  position: absolute;
  top: 0rpx;
}

.bottom-center-wx {
  height: 51rpx;
  width: 51rpx;
  margin-right: 18rpx
}

.share-tv {
  color: #fff;
  font-size: 35rpx;
}
