}

.tixianview_discription_upperlimit {
  margin:20rpx;
  color: #666;
  font-size: 30rpx;
  text-align: center;
}
.tixianview_discription_upperlimit_noTitle {
  margin-left:20rpx;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  margin-top: 35rpx;
  color: #666;
  font-size: 30rpx;
  text-align: center;
}

