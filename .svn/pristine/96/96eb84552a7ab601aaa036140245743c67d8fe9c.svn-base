/* @import "wxSearch/wxSearch.wxss"; */
@import "../../../common/category-template/category-template.wxss";
::-webkit-scrollbar{
  height: 0;
  width: 0;
  color: transparent;
}
.search-container {
  display: flex;
  /* justify-content: space-between; */
  align-items: center;
  color: #fff;
  height: 50rpx;
  width: 100%;
  padding: 2% 0;
  /* background-color: greenyellow; */
}

.typeimage {
  display: flex;
  width: 50rpx;
  margin-left: 20rpx;
}
.moneyimage {
  display: flex;
  width: 50rpx;
  right: 20rpx;
  position: absolute;
}

.wxSearch-input {
  display: flex;
  margin-left: 40rpx;
}
.search-container icon {
  display: flex;
  margin-right: 40rpx;
}


.brandsContainer {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  /* position: relative; */
  padding-top: 10rpx;
  margin-right: 2%;
  padding-bottom: 0;
}

.brandsContainer-item {
  position: relative; 
  flex: 0 0 23%; 
  margin-left: 2%;
  /* background-color: red; */
  text-align: center;
  height: 160rpx;
  margin-bottom: 3%;
  border-radius: 8rpx;
}
.brandsContainer-item image {
  width: 100%;
  height: 160rpx;
  border-radius: 8rpx;
}
.moreBrand {
  display: flex;
  flex-direction: column;
  justify-content: center;
  background-color: rgb(248, 248, 248);
  color: rgb(62, 62, 62);
  height: 170rpx;
  font-size: 28rpx;
  border-radius: 8rpx;
}

.typeContainer {
  position: relative;
  z-index: 1;
  /* padding-top: 100rpx; */
}

.wxSearch-section {
  background-color: #fff;
  padding: 5px 0px;
  border: 1px #eee solid;
  position: fixed;
  /* width: 100%; */
  flex: none;
  left: 0;
  right: 0;
  z-index: 5;
  top: 0;
}

.wxSearch-pancel {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  background-color: #eee;
  color: #a8a8a8;
  margin-left: 12%; 
  width: 76%;
  height: 80rpx;
  border-radius: 8rpx;
  position: absolute;
}

