/* pages/sign/shareQshop/shareQshop.wxss */
@import "../../../common/listHomeTemplate/listtemplate.wxss";
@import "../../../common/signfinish-template/signfinish.wxss";
.share-q-shop{
  justify-content: center;
  display: flex;
}
::-webkit-scrollbar{
  height: 0;
  width: 0;
  color: transparent;
}
.shoplist {
  width: 100%;
  height: 100%;
  margin-bottom: 160rpx;
  background: #F0F0F0;
  margin-top: -8rpx;
}
.list-all {
  margin-top: 10px;
  padding-left: 2%;
  padding-right: 2%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
}
.select_icon{
  position: absolute;
  width: 50rpx;
  height:50rpx;
  padding: 20rpx;
  right: 10rpx;
  top: 10rpx;
  display: flex;
}

.no-data{
  width: 100%;
  height: 100%;
  margin-bottom: 200rpx;
  background: white;
  position: fixed;
  z-index: -999;
  top: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}
.no-data text{
  font-size: 40rpx;
  color : #A8A8A8;
}



.bottom-share-view-btn{
  width: 100%;
  height: 160rpx;
  position: fixed;
  background: white;
  display: flex;
  flex-direction: column;
  bottom: 0;
  opacity: 0;
  z-index: 2;
}

.bottom-share-view-icon{
  width: 100%;
  height: 160rpx;
  position: fixed;
  background: white;
  display: flex;
  flex-direction: column;
  bottom: 0;
}
.bottom-share-view-line{
  width: 100%;
  height: 1px;
  background:#C5C5C5;
}

.weixin-share-btn-share{
  width: 420rpx;
  height: 90rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  border: 1rpx solid #76C231;
  background-color: #76C231;
  border-radius: 6rpx;
  align-self: center;
  margin-top: 35rpx;
}
.weixin-share-icon-text{
  font-size: 26rpx;
  color: #FFFFFF;
  font-weight: 700;
} 
 .weixin-share-icon{
  width:60rpx;
  height: 60rpx;
  margin-right: 20rpx;
}
 .weixin-share-icon-btnX{
  width: 420rpx;
  height: 100rpx;
  opacity: 0;
  align-self: center;
  margin-top: 35rpx;
} 



 .tv-name-showsub {
  margin-top: 0px;
  margin-left: 5px;
  color: #3e3e3e;
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

