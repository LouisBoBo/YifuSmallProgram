      <image class='closeimage-shareback' src="/iconImages/icon_close.png" catchtap='closeRedPackageShareBack'></image>


      <!-- <image class='user-pic' src="{{headPic}}" ></image> -->

      <image class="user-pic" src="{{shareBackheadpic}}" mode="aspectFill" />


      <button class='button_confrim_upperlimit' bindgetuserinfo='onclick' open-type="getUserInfo" form-type='submit'></button>
    </view>
  </form>
</template>