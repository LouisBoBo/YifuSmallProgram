.open-yifu-root-tx {
  width: 580rpx;
  background-color: #fff;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
  padding-top: 20rpx;
}

.open-yifu-text {
  align-self: center;
  font-size: 32rpx;
  color: rgb(66, 66, 66);
  margin-left: 10rpx;
  margin-right: 10rpx;
}

.open-yifu-text-red {
  font-size: 36rpx;
  color: #ff3f8b;
}

