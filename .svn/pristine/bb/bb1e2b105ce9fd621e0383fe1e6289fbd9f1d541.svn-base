.open-yifu-bottom-btn {
  height: 80rpx;
  width: 520rpx;
  font-size: 32rpx;
  text-align: center;
  line-height: 80rpx;
  color: #fff;
  background-color: #ff3f8b;
  border-radius: 3px;
  align-self: center;
  margin-top: 40rpx;
  margin-bottom: 40rpx;
}
.img-close-tx{
  width: 40rpx;
  height: 40rpx;
  margin-left: 500rpx
}


.img-app-icon{
  width: 270rpx;
  height: 240rpx;
  align-self: center  
}

