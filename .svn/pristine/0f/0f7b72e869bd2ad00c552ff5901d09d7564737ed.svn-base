/* 转盘动态添加 动画属性 */

.animate {
  -webkit-animation: circle 5.5s linear infinite;
  -moz-animation: circle 5.5s linear infinite;
  animation: round 5.5s linear infinite;
}

/* 动画样式设置 */

@keyframes round {
  100% {
    transform: rotate(1turn);
  }
}


