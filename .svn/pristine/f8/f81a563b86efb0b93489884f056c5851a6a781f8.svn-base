.toast_content_box {
  display: flex;
  width: 100%;
  height: 100%;
  justify-content: center;
  align-items: center;
  position: fixed;
  z-index: 9999;
  top: 0;
}

.toast_content {
  padding: 40rpx;
  background: rgba(0, 0, 0, 0.8);
  border-radius: 20rpx;
  margin-left: 13%;
  margin-right: 13%;
}

.toast_content_text {
  height: 100%;
  color: #fff;
  font-size: 32rpx;
  text-align: left;
}
