  padding-top: 10rpx;
  color: #a8a8a8;
  font-size: 22rpx;
  height: 100rpx;
}
.friends-bottom {
  display: flex;
  flex-direction: row;
  justify-content: space-between; 
  align-items:center; 
  font-size:26rpx;
  margin-left: 20rpx;
  margin-top: 10rpx;
  height: 40rpx;
}
.friends-bottom-rightMoney {
  display: flex;
  padding-right: 20rpx; 
