.status{
  display: flex;
  flex-direction: row;
}
.status-image{
  /* margin: 20rpx 0rpx 20rpx 150rpx; */
  height: 50rpx;
  width: 50rpx;
  /* background-color: firebrick; */
}
.status-line{
  margin-top: 45rpx;
  height: 2rpx;
  width: 150rpx;
  background-color: #666;
}
.statusTitle{
  display: flex;
  flex-direction: row;
}
.statusTitle-text{
  width: 150rpx;
  text-align: center;
  color: #666;
  font-size: 30rpx;
}

.tixianview {
  margin-top: 30rpx;
  margin-left: 35%;
  width: 30%;
  text-align: center;
  line-height: 50rpx;
  overflow: hidden;
  justify-content: center;
  border: 1rpx solid #ff3f8b;
  border-radius:2px;
  color: #ff3f8b;
  font-size: 30rpx;
}
.tixianagain{
  margin-top: 50rpx;
  margin-left: 10%;
  width: 80%;
  text-align: center;
  line-height: 80rpx;
  overflow: hidden;
  justify-content: center;
  background-color: #ff3f8b;
  border-radius:8rpx;
  font-size: 32rpx;
  color: white;
}
.supportCardView{ 
   margin: 20rpx 30rpx 20rpx 30rpx;
   height: 80rpx;
   border-bottom: 1px solid #eee;
 } 

