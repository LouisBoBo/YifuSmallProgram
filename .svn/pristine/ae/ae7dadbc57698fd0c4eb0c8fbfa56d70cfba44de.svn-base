}

.videoView{
  display: flex;
  justify-content: center;
  margin-top: 14rpx;
  margin-bottom: 65rpx;
	width: 627rpx;
  height: 424rpx;
	position: relative;
  background: rebeccapurple;
}
.video{
  width: 627rpx;
  height: 424rpx;
  margin: 0;
}

.videoImg{
  width: 627rpx;
  height: 424rpx;
	position: absolute;
	top: 0;
	left: 0;
	z-index: 99;
}

.rawardview {
