}

.zw-tabs-item {
  flex-direction: column;
  display: flex;
  flex: 1;
  align-items: center;
  padding: 0 0 0 0;
}

.zw-tabs-item image {
  width: 120rpx;
  height: 120rpx;
}

.zw-tabs-item text {
  color: #666;
  font-size: 30rpx;
  padding: 28rpx 0 0 0;
  line-height: 30rpx;
}

.rawardImg{
  width: 100%;
}
.bottomview{
  display: flex;
