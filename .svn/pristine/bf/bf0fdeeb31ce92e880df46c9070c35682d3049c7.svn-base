.tobar {
  margin-top: 20rpx;
}

.head-top {
  width: 100%;
  display: flex;
  flex-direction: column;
}

.head-view {
  width: 100%;
}

.head-view image {
  height: 100%;
  width: 100%;
}

.head-text {
  display: flex;
  flex-direction: column;
  margin-left: 20rpx;
  margin-right: 20rpx;
}

.indiana-status {
  display: flex;
  flex-direction: row;
}

.tv-status {
  color: #6dc264;
  font-size: 25rpx;
  border: 1px #6dc264 solid;
  height: 35rpx;
  padding-left: 10rpx;
  padding-right: 10rpx;
  border-radius: 8rpx;
}

.tv-status-end {
  color: #ff3f8c;
  font-size: 25rpx;
  border: 1px #ff3f8c solid;
  height: 35rpx;
  padding-left: 10rpx;
  padding-right: 10rpx;
  border-radius: 8rpx;
}

/* 商品名字 */

.tv-shop-name {
  margin-left: 20rpx;
  color: #000;
  font-size: 30rpx;
  font-weight: 570;
}

.tv-shop-explain {
  margin-top: 10rpx;
  color: #7d7d7d;
  font-size: 25rpx;
}

.head-view-price {
  position: relative;
  margin-top: 5rpx;
  display: flex;
  flex-direction: row;
}

.tv-shop-se-price {
  /* 价格 */
  color: #ff3f8b;
  font-size: 30rpx;
}

/* 专柜价 */

.shop-price-line {
  position: relative;
  margin-left: 4rpx;
  display: flex;
  align-items: flex-end;
}

.tv-shop-price {
  color: #a8a8a8;
  font-size: 25rpx;
}

.tv-line {
  position: absolute;
  bottom: 15rpx;
  height: 2rpx;
  width: 50rpx;
  background-color: #a8a8a8;
}

.issue_code {
  color: #7d7d7d;
  font-size: 25rpx;
}

.price-issue-code {
  display: flex;
  flex-direction: column;
}

.v-center {
  position: relative;
  display: flex;
  flex-direction: row;
}

.share-notic-image {
  position: absolute;
  right: 0;
  height: 80rpx;
  width: 320rpx;
}

.share-notic-image image {
  height: 100%;
  width: 100%;
}

.head_line {
  margin-top: 15rpx;
  width: 100%;
  height: 2rpx;
  background-color: #f0f0f0;
}

.head_line_short {
  margin-right: 20rpx;
  margin-left: 20rpx;
  margin-top: 15rpx;
  width: 100%;
  height: 2rpx;
  background-color: #f0f0f0;
}

.head-bottom {
  margin-left: 20rpx;
  margin-right: 20rpx;
  display: flex;
  flex-direction: column;
}

.progress-long {
  margin-top: 5rpx;
}

.people-count {
  width: 100%;
  margin-top: 5rpx;
  position: relative;
  display: flex;
  flex-direction: row;
}

.people-left {
  font-size: 25rpx;
  color: #a8a8a8;
}

.people-right {
  font-size: 25rpx;
  color: #a8a8a8;
  position: absolute;
  right: 0;
  display: flex;
  flex-direction: row;
}

.right-tv {
  font-size: 25rpx;
  color: #a8a8a8;
}

.right-num {
  font-size: 25rpx;
  color: #ff3f8b;
}

.v-take-number {
  margin-top: 15rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
}

.own-no-take {
  color: #a8a8a8;
  font-size: 25rpx;
}

.own-take {
  display: flex;
  flex-direction: column;
}

.own-take-count {
  font-size: 25rpx;
  color: #ff3f8b;
}

.own-take-number {
  font-size: 25rpx;
  color: #ff3f8b;
}

.own-take-count2 {
  font-size: 25rpx;
  color: #a8a8a8;
}

.own-take-number2 {
  font-size: 25rpx;
  color: #a8a8a8;
}
.own-take-tv2{
 font-size: 25rpx;
  color: #ff3f8b;
}

.take-people {
  margin-top: 15rpx;
  font-size: 28rpx;
  color: #ff3f8b;
}

/* 开奖揭晓---- start */

.head-bottom-result {
  margin-left: 20rpx;
  margin-right: 20rpx;
  display: flex;
  flex-direction: column;
  margin-top: 20rpx;
}

.head-bottom-title {
  display: flex;
  flex-direction: row;
  height: 50rpx;
  width: 100%;
  align-items: center;
  background-color: #ff3f8b;
}

.head-bottom-title-pic {
  margin-left: 20rpx;
  height: 30rpx;
  width: 30rpx;
}

.head-bottom-title-num {
  margin-left: 30rpx;
  font-size: 22rpx;
  color: #fff;
}

.head-bottom-winners {
  background-color: #fdf6eb;
  display: flex;
  flex-direction: row;
  height: 100rpx;
  width: 100%;
  align-items: center;
}

.head-bottom-winners-pic {
  margin-left: 20rpx;
  height: 70rpx;
  width: 70rpx;
  border-radius: 30rpx;
}

.head-bottom-winners-out {
  margin-left: 15rpx;
}

.head-bottom-winners-out-winner {
  display: flex;
  flex-direction: row;
}

.head-bottom-winners-tv {
  color: #ff3f8b;
  font-size: 25rpx;
}

.head-bottom-winners-name {
  margin-left: 10rpx;
  color: #3e3e3e;
  font-size: 25rpx;
}

.head-bottom-winners-time {
  margin-top: 10rpx;
  color: #a8a8a8;
  font-size: 22rpx;
}

/* 开奖揭晓---- end */

.detail-images {
  width: 100%;
}

.detail-images image {
  width: 100%;
}

/* 参与记录 */

.record {
  /* margin-left: 20rpx;
  margin-right: 20rpx; *//* margin-top: 30rpx; */
}

.record-top {
  margin-left: 20rpx;
  margin-top: 15rpx;
  display: flex;
  flex-direction: row;
  position: relative;
}

.record-top-tv {
  font-size: 25rpx;
  color: #3e3e3e;
}

.record-top-date {
  display: flex;
  position: absolute;
  right: 20rpx;
  font-size: 25rpx;
  color: #a8a8a8;
}

.record-item {
  position: relative;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.record-item-line {
  height: 2rpx;
  width: 100%;
  background-color: #f0f0f0;
  margin-bottom: 15rpx;
  margin-top: 15rpx;
}

.record-item-pic {
  margin-left: 10rpx;
  height: 80rpx;
  width: 80rpx;
  border-radius: 40rpx;
}

.record-item-center {
  margin-left: 20rpx;
  display: flex;
  flex-direction: column;
}

.record-item-center-name {
  color: #3e3e3e;
  font-size: 25rpx;
}

.record-item-center-date {
  margin-top: 10rpx;
  color: #a8a8a8;
  font-size: 25rpx;
}

.record-item-right {
  position: absolute;
  right: 20rpx;
  display: flex;
  flex-direction: row;
}

.record-item-right-num {
  color: #ff3f8b;
  font-size: 25rpx;
}

.record-item-right-people {
  color: #a8a8a8;
  font-size: 25rpx;
}

.record-white {
  height: 300rpx;
  width: 100%;
  background-color: #fff;
}

.rule-title {
  width: 100%;
  display: flex;
  justify-content: center;
  margin-top: 30rpx;
}

.rule-title-image {
  height: 40rpx;
  width: 240rpx;
}

.rule-item {
  margin-left: 20rpx;
  margin-right: 20rpx;
  margin-top: 30rpx;
  font-size: 28rpx;
  color: #7d7d7d;
}

.rule-white {
  height: 300rpx;
  width: 100%;
  background-color: #fff;
}

.detail-all {
  margin-bottom: 80rpx;
}

.bottom_bar {
  height: 80rpx;
  width: 100%;
  background-color: #fff;
  position: fixed;
  bottom: 0;
  display: flex;
  flex-direction: row;
}

.bottom-left {
  height: 100%;
  width: 50%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  position: relative;
}
.lin-custom {
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  z-index: 5;
  align-items: center;
  overflow-x: hidden;
  opacity: 0;
}

.bottom-left-pic {
  height: 35rpx;
  width: 35rpx;
}

.bottom-left-pic image {
  height: 100%;
  width: 100%;
}

.custerm-service-tv {
  margin-top: 10rpx;
  color: #999;
  font-size: 20rpx;
}

.bottom-right {
  height: 100%;
  width: 50%;
  background-color: #ff3f8b;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.bottom-right-end {
  height: 100%;
  width: 50%;
  background-color: #c5c5c5;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.bottom-right-tv {
  color: #fff;
  font-size: 28rpx;
}

.selection-s {
  background: rgba(0, 0, 0, 0.7);
  z-index: 10;
  position: fixed;
}

.selection-no-data {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10;
  /* background: rgba(0, 0, 0, 0.7); */
}

.selection-no-data image {
  height: 100%;
  width: 100%;
}

.selection-touch-look {
  position: fixed;
  top: 40%;
  right: 0;
}

.image-touch-look {
  width: 70rpx;
}

.selection-share {
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.7);
  z-index: 10;
  position: fixed;
}

.dialog-share {
  background-color: #fff;
  top: 13%;
  position: fixed;
  left: 15%;
  right: 15%;
  border-radius: 10rpx;
}

/* 抽奖记录与往期揭晓， */

.top_record_fixed {
  position: fixed;
  z-index: 8;
  right: 20rpx;
  top: 20rpx;
  display: flex;
  flex-direction: row;
}

.fixed_record {
  height: 50rpx;
  width: 140rpx;
  border-radius: 5rpx;
  background-color: #f9243d;
  color: #fff;
  font-size: 25rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.fixed_announce{
  height: 50rpx;
  width: 140rpx;
  border-radius: 5rpx;
  background-color: #f9243d;
  color: #fff;
  font-size: 25rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 20rpx;
}