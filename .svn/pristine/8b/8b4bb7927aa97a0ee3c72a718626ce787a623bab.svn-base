  justify-content: center;
  align-items: center;
}
.fightview{
  margin-top: 20rpx;
  height: 60rpx;
  border-width: 0rpx 0 1px 0;
  border-color: #f0f0f0;
  border-style: solid;
}
.fightview-title{
  float: left;
  display: flex;
  flex-direction: row;
}
.fightview-title-text{
  margin-top: 5rpx;
  margin-left: 20rpx;
  font-size: 28rpx;
  color:rgb(62, 62, 62);
}
.fightview-title-image{
  width: 50rpx;
  height: 50rpx;
  border-radius: 25rpx;
}
.fightview-tuanzhang-image{
  margin-top: -10rpx;
  margin-left: 210rpx;
  width: 50rpx;
  height: 30rpx;
  border-radius: 15rpx;
  position: absolute;
}
.fightview-title-content{
  margin-top: 5rpx;
  margin-left: 20rpx;
  font-size: 24rpx;
  color: #666;
}
.fightview-content{
  float: right;
  margin-top: 5rpx;
  margin-right: 20rpx;
  font-size: 24rpx;
  color:rgb(125, 125, 125);
}

.footview-pay{
  height: 40rpx;
}
.shopview-pay{
  float: right;
  font-size: 28rpx;
  color: #666;
  margin-right: 10rpx;
  margin-bottom: 5rpx;
}

