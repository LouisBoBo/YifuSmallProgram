}

.footview {
  position: fixed;
  bottom: 20rpx;
  width: 100%;
}

.footview-invite {
  background-color: #ff3f8b;
  font-size: 32rpx;
  color: white;
  height: 70rpx;
  border-radius: 5rpx;
