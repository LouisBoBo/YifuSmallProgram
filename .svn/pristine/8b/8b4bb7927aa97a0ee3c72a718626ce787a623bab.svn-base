  flex-direction: row;
}

.headView-time-text {
  font-size: 30rpx;
  color: #666;
  display: flex;
  align-items: center;
}
.headView-Img{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  margin-top: 30rpx;
}
