/* pages/sign/inviteFriends/memberFriendsReward.wxss */
.notEnoughYidou-root {
  width: 100%;
  /* background-color: #fd215a; */
  /* border-radius: 15rpx; */
  display: flex;
  flex-direction: column;
  position: relative;
  padding-bottom: 35rpx;
  border-bottom: 20rpx solid #f0f0f0;
  border-top: 1rpx solid #f0f0f0;
}

.notEnough-dialog-title {
  align-self: center;
  font-size: 30rpx;
  color: #3e3e3e;
  margin-top: 30rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
}
.notEnough-dialog-title2 {
  align-self: center;
  font-size: 30rpx;
  color: #ff3f8b;
  margin-top: 35rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
}

.notEnough-dialog-content {
  align-self: center;
  font-size: 30rpx;
  color: #3e3e3e;
  /* margin-top: 0; */
  margin-left: 30rpx;
  margin-right: 30rpx;
  /* background-color: rebeccapurple; */
}

.notEnough-dialog-myReward {
  display: flex;
  flex-direction: row;
  align-self: center;
}

.notEnough-dialog-myRewardContent {
  align-self: center;
  align-items: center;
  text-align: center;
  font-size: 30rpx;
  color: #ff3f8b;
  margin-top: 5rpx; 
  margin-left: 50rpx;
  margin-right: 50rpx;
}

.middleLine {
  margin-top: 30rpx;
  height: 60rpx;
  width: 1px;
  background-color: #ff3f8b;
}

.friendTodayReward {
  font-size: 30rpx;
  color: #3e3e3e;
  width: 100%;
  text-align: center;
  margin-top: 20rpx;
  padding-bottom: 20rpx;
  border-bottom: 1rpx solid #f0f0f0;
}

.friendsReward {
  display: flex;
  margin-top: 20rpx;
  padding-bottom: 20rpx;
  border-bottom: 1rpx solid #f0f0f0;
  align-items: center;
}
.friends-headImg {
  margin-left: 20rpx;
  width: 100rpx;
  height: 100rpx;
  /* background-color: rebeccapurple; */
  border-radius: 50rpx;
}
.friends-left {
  margin-left: 20rpx;
  font-size: 30rpx;
  color: #ff3f8b;
  display: flex;
  flex-direction: column;
  width: 53%;
}
.friends-right{
  width: 22%;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  background-color: #ff3f8b;
  color: white;
  font-size: 26rpx;
  border-radius: 8rpx;
}
.friends-top {
  display: flex;
  flex-direction: row;
}
.friends-top-name{
  font-size: 30rpx;
  color: #3e3e3e;
  width: 200rpx;
}
.friends-top-time {
  margin-right: 10rpx;
  width: 200rpx;
  color: #a8a8a8;
}
.friends-bottom {
  display: flex;
  flex-direction: row;
  justify-content: space-between;  
}
.friends-bottom-rightMoney {
  display: flex;
  padding-right: 20rpx; 
  text-align: right;
  background-color: #ff3f8b;
  color: white;
}

.emptyView {
  padding-top: 40%;
  text-align: center;
  font-size: 30rpx;
  color: #666666;
}
