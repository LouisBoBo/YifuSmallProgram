button {
    margin: 0;
    padding: 0;
    border-radius: 0;
    background-color: rgba(0,0,0,0);
    overflow: visible;
    text-align: left;
}

button::after {
    border: none;
}

.page {
    padding: 0 37rpx 0 38rpx;
    box-sizing: border-box;
}

