.list-all {
  margin-top: 10px;
  padding-left: 2%;
  padding-right: 2%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
}

.v-item {
  position: relative;
  width: 49%;
}

.button {
  top: 0;
  width: 100%;
  height: 100%;
  position: absolute;
  /* background-color: goldenrod; */
  opacity: 0;
}

.image-item {
  position: relative;
}

.image-pic {
  height: 100%;
  width: 100%;
}

.v-bottom {
  /* width: 100%;
  position: absolute;
  bottom: -10rpx;
  display: flex;
  flex-direction: column; */
  width: 100%;
  position: absolute;
  bottom: 0rpx;
  display: flex;
  flex-direction: column;
}

.v-text {
  margin-top: 15rpx;
  background-color: #fff;
  width: 100%;
  display: flex;
  flex-direction: column;
}

.v-suppley {
  width: 100%;
  display: flex;
  justify-content: center;
}

.tv-suppley {
  margin-left: 20rpx;
  margin-right: 20rpx;
  opacity: 0.4;
  background-color: #000;
  display: flex;
  border-radius: 18rpx;
  justify-content: center;
  align-items: center;
  height: 36rpx;
  padding-right: 10px;
  padding-left: 10px;
  color: #fff;
  font-size: 10px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

