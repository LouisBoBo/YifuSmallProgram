.tv-name {
  display: flex;
  flex-direction: column;
  justify-content: center;
  height: 80rpx;
  margin-top: 0px;
  margin-left: 5px;
  color: #3e3e3e;
  font-size: 24rpx;
}

.tv-price-all {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-top: 3px;
  margin-left: 5px;
  margin-right: 5px;
  margin-bottom: 5rpx;
}

.tv-price {
  font-size: 35rpx;
  color: red;
  font-weight: 700;
}

.tv-price-shoppe {
  display: flex;
  font-size: 28rpx;
  color: #9a9a9a;
  align-self: center;
}

.tv-kickback {
  margin-top: 3px;
  margin-left: 5px;
  margin-bottom: 4px;
  font-size: 28rpx;
  color: #3e3e3e;
}

.discountview-images {
  position: absolute;
  top: 0;
  left: 0rpx;
  height: 70rpx;
  width: 70rpx;
}

.discountview-image-texts {
  position: absolute;
  top: 10rpx;
  height: 70rpx;
  width: 70rpx;
  overflow: hidden;
  color: white;
  font-size: 25rpx;
  display: flex;
  justify-content: center;
}

.discountview-titles {
  margin-left: 155rpx;
  height: 40rpx;
  width: 140rpx;
  background: #ff3f8b;
  text-align: center;
  /* line-height: 40rpx; */
  overflow: hidden;
  background-color: #ff3f8b;
  color: white;
  font-size: 25rpx;
  position: absolute;
  top: 0;
  right: 0rpx;
}

 .tv-name-showsub {
  margin-top: 0px;
  margin-left: 5px;
  margin-bottom: 5rpx;
  color: #3e3e3e;
  font-size: 26rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
