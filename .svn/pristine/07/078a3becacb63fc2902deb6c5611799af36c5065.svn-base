      var shareJson = '快来' + this.data.wxcx_shop_group_price + '元拼' + '【' + supp_label + '正品' + data.type2 + '】,' + '专柜价' + shop.shop_price + '元!';
      // if (shop_type == 2) {
      //   shareJson = '快来' + this.data.wxcx_shop_group_price + '元拼' + '【' + this.data.shop.shop_name + '】,' + '原价' + shop.shop_price + '元!';
