          that.setData({ //刷新列表
            currentpage: 1,
          })
          var shop = orderItem.orderShops[0];
          if (shop.shop_from != 10 && shop.shop_from != 11 && shop.shop_from != 4 && shop.shop_from != 6)
          {
            //普通订单到订单详情界面
