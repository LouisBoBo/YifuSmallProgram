<view class="drawer_screen_withdraw" catchtouchmove="preventTouchMove" wx:if="{{showBuQaunZaunShi}}">
  <template is='buquanZuanshiTemplate' data='{{Upyun,zuanshiDaoJishiStr}}'></template>
</view>






