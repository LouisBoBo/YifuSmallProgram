.list-all {
  padding-left: 2%;
  padding-right: 2%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  background: #F0F0F0;
  padding-top: 8px;
}

.v-item {
  position: relative;
  width: 49%;
  margin-bottom: 8rpx;
}


.image-item image {
  height: 100%;
  width: 100%;
}

.v-bottom {
  width: 100%;
  position: absolute;
  bottom: 6rpx;
  display: flex;
  flex-direction: column;
  margin-top: 15rpx;
  background-color: #FFF;
}

