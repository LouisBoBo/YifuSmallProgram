/* pages/mine/Coupon/Coupon.wxss */
@import "../../../common/Coupon-template/coupon.wxss";
@import "../../../common/Coupon-template/coupon-pop.wxss";
@import "../../../common/Login-template/loginPop.wxss";


.taskShuoming-image {
  width: 55rpx;
  height: 160rpx;
  position: fixed;
  background: #ff3f8b;
  right: 0;
  top: 200rpx;
  z-index: 99;
  display: flex;
}
.taskShuoming-image-text{
  margin-left: 12rpx;
  font-size: 28rpx;
  color: white;
}
.stv-container {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.withAnimate {
  transition: all 100ms ease;
  -webkit-transform: translate3d(0, 0, 0);
  -moz-transform: translate3d(0, 0, 0);
  -ms-transform: translate3d(0, 0, 0);
  transform: translate3d(0, 0, 0);
  -webkit-backface-visibility: hidden;
  -moz-backface-visibility: hidden;
  -ms-backface-visibility: hidden;
  backface-visibility: hidden;
  -webkit-perspective: 1000;
  -moz-perspective: 1000;
  -ms-perspective: 1000;
  perspective: 1000;
}
.stv-container .tab-bar {
  position: relative;
  display: flex;
  font-size: 30rpx;
  color: #666666;
}
.stv-container .tab-bar .tab-active {
  color: #ff3f8b;
}
.stv-container .tab-bar .tab {
  display: flex;
  align-items: center;
  justify-content: center;
  padding-top: 16rpx;
  padding-bottom: 20rpx;
}
.stv-container .tab-bar .under-line {
  position: absolute;
  bottom: 0;
  height: 6rpx;
}
.stv-container .scroll-view {
  position: relative;
  width: 100%;
  height: 100%;
  background: #e7eaef;
}
.stv-container .scroll-view .scroll-view-wrapper {
  position: absolute;
  top: 0;
  bottom: 0;
  display: flex;
}
.stv-container .scroll-view .scroll-view-wrapper .one-scene {
  height: 100%;
}
.one-scene couponview {
  padding-top: 250rpx;
}

.emptyView {
  padding-top: 50%;
  text-align: center;
  font-size: 30rpx;
  color: #666666;
}