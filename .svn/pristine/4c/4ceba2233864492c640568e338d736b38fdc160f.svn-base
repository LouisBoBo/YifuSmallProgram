<!--余额抵扣弹框-->
<view class="drawer_screen_withdraw" catchtouchmove="preventTouchMove" wx:if="{{moneyDiscountShowFlag}}">
  <template is='open-Discount' data='{{moneyDiscount,oneYuanDiscriptionTitle}}'></template>
</view>