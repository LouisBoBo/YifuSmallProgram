/*推送模板消息跳转界面
赚钱任务： pages/sign/sign
帐户明细： pages/mine/wallet/accountDetail/accountDetail？messagefrom=true  
*/
@import "/common/toastTest/toastTest.wxss";

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.drawer_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 998;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
}

.drawer_screen_withdraw {
  display: flex;
  width: 100%;
  height: 100%;
  justify-content: center;
  align-items: center;
  position: fixed;
  z-index: 998;
  top: 0;
  background-color: rgba(0, 0, 0, 0.6);
}

.drawer_screen_transparent{
  display: flex;
  width: 100%;
  height: 100%;
  justify-content: center;
  align-items: center;
  position: fixed;
  z-index: 998;
  top: 0;
  background-color: rgba(0, 0, 0, 0); 
}

